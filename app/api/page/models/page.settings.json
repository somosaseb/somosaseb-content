{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Page",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 140
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.banner",
        "content.content"
      ]
    },
    "slug": {
      "type": "string",
      "unique": true
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "meta.head"
    }
  }
}
